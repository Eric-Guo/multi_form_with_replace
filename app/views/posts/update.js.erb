document.getElementById("post-<%= @post.id -%>-title").innerHTML = '<%= j text_field_tag "post[title]", @post.title, id: "post-#{@post.id}-title" -%>';
document.getElementById("post-<%= @post.id -%>-content").innerHTML = '<%= j text_field_tag "post[content]", @post.content, id: "post-#{@post.id}-content" -%>';
